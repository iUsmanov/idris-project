{
	"name": "idris-project",
	"version": "1.0.0",
	"description": "",
	"main": "index.js",
	"scripts": {
		"start": "webpack serve --env port=3000",
		"start:server": "node ./json-server/index.js",
		"build:prod": "webpack --env mode=production apiURL=production.ru",
		"build:dev": "webpack --env mode=development",
		"analyze:prod": "npm run build:prod --env analyze=true apiURL=production.ru",
		"analyze:dev": "npm run build:dev --env analyze=true",
		"lint:ts": "eslint \"**/*.{ts,tsx}\"",
		"lint:ts:fix": "eslint \"**/*.{ts,tsx}\" -- fix",
		"lint:style": "stylelint \"**/*.{css,scss}\"",
		"lint:style:fix": "stylelint \"**/*.{css,scss}\" -- fix",
		"pret": "prettier --check **/*.{ts,tsx,js,json}",
		"pret:fix": "prettier --write **/*.{ts,tsx,js,json}",
		"test:unit": "jest --config ./config/jest/jest.config.ts",
		"test:ui": "npx loki test",
		"test:ui:ci": "npx loki --requireReference --reactUri file:./storybook-static",
		"test:ui:ok": "npx loki approve",
		"test:ui:update": "npx loki update",
		"test:ui:report": "npm run test:ui:json && npm run test:ui:html",
		"test:ui:json": "node scripts/generate-visual-json-report.js",
		"test:ui:html": "reg-cli --from .loki/report.json --report .loki/report.html",
		"storybook": "storybook dev -p 6006 -c ./config/storybook",
		"storybook:build": "storybook build -c ./config/storybook",
		"prepare": "husky install"
	},
	"author": "",
	"license": "ISC",
	"devDependencies": {
		"@babel/core": "^7.21.8",
		"@babel/preset-env": "^7.21.5",
		"@babel/preset-react": "^7.18.6",
		"@babel/preset-typescript": "^7.21.5",
		"@pmmmwh/react-refresh-webpack-plugin": "^0.5.10",
		"@storybook/addon-essentials": "^7.0.12",
		"@storybook/addon-interactions": "^7.0.12",
		"@storybook/addon-links": "^7.0.12",
		"@storybook/blocks": "^7.0.12",
		"@storybook/react": "^7.0.12",
		"@storybook/react-webpack5": "^7.0.12",
		"@storybook/testing-library": "^0.0.14-next.2",
		"@svgr/webpack": "^8.0.1",
		"@testing-library/jest-dom": "^5.16.5",
		"@testing-library/react": "^14.0.0",
		"@testing-library/user-event": "^14.4.3",
		"@types/jest": "^29.5.1",
		"@types/node": "^18.16.3",
		"@types/react": "^18.2.5",
