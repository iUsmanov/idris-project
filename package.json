		"i18next-browser-languagedetector": "^7.0.1",
		"i18next-http-backend": "^2.2.0",
		"react": "^18.2.0",
		"react-dom": "^18.2.0",
		"react-i18next": "^12.2.2",
		"react-redux": "^8.0.5",
		"react-router-dom": "^6.11.1"
	},
	"loki": {
		"configurations": {
			"chrome.laptop": {
				"target": "chrome.app",
				"width": 1366,
				"height": 768,
				"deviceScaleFactor": 1,
				"mobile": false
			},
			"chrome.iphone7": {
				"target": "chrome.app",
				"preset": "iPhone 7"
			}
		}
	},
	"lint-staged": {
		"**/*.{ts,tsx}": "eslint",
		"**/*.{css,scss}": "stylelint"
	}
}
